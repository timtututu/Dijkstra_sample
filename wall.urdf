<?xml version="1.0" ?>
<robot name="simple_box">
  <link name="my_box">
    <inertial>
      <origin xyz="0 0 0.5" /> 
      <mass value="10.0" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="1.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <visual>
      <origin xyz="0 0 0.5"/>
      <geometry>
        <box size="1 1 1" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.5"/>
      <geometry>
        <box size="1 1 1" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="my_box">
    <material><Gazebo/>
    <Black></Black></material>
  </gazebo>
</robot>